{
  "version": 2,
  "name": "avionti",
  "builds": [
    {
      "src": "PicToPDF/**",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/PicToPDF/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/PicToPDF/(css|js)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/PicToPDF/",
      "permanent": false
    },
    {
      "source": "/app",
      "destination": "/PicToPDF/#download",
      "permanent": false
    },
    {
      "source": "/download",
      "destination": "/PicToPDF/#download",
      "permanent": false
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}